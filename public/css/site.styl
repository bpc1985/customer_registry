body
    padding-top 61px

.navbar-inverse
    background-color: #95ba00;
    border-color: #95ba00;

.navbar-header a.navbar-brand
    margin-top -15px
    margin-bottom -20px
    h6
        color #ccc

.btn
    padding: 6px 20px

.btn-info, .btn-primary
    background-color: #95ba00;
    border-color: #95ba00;
    border-radius: 5px;

.btn-default
    border-radius: 5px;

span.glyphicon
    margin-right 5px;

footer
    padding-left 15px
    padding-right 15px
    p
        border-top 1px solid #d7e0e2

.navbar-form
    .form-group
        margin-left 10px
    button
        margin-left 10px

.toast-top-right
    top 70px

.toast-success
    background-color #95ba00

.topspace
    margin-top 30px

.choose_time table
    margin-left: 15px;
    input
        width: 60px !important;

hr
    border-color: #95ba00
    border-style: solid none
    border-width: 2px 0
    margin: 18px 0

.form-group.required .control-label:before
    content: "*"
    color: red

.add-zipcode-btn
    padding: 6px 12px
    width: 75px

ul.zipcode-list
    list-style: none
    padding: 0

li.zipcode-choice
    border: 1px solid #aaaaaa
    border-radius: 3px
    height: 30px
    width: 75px
    margin-bottom: 10px
    margin-right: 5px;
    padding-top: 4px
    text-align: center
    color: #6f6f6f
    float: left
    a.zipcode-close
        color: black
        margin-left: 5px;
